{
  "name": "doubleleft/hook",
  "description": "backend as a service",
  "keywords": ["hook", "doubleleft"],
  "license": "MIT",
  "authors": [{
    "name": "Endel Dreyer",
    "email": "edreyer@doubleleft.com"
  }],
  "autoload": {
    "classmap": ["src"],
    "psr-0": {}
  },
  "require": {
    "php": ">=5.3.0",
    "composer/composer": "dev-master",
    "slim/Slim": "dev-master",
    "doctrine/dbal" : "2.*",
    "illuminate/database": "5.0.*",
    "illuminate/config": "5.0.*",
    "illuminate/cache": "5.0.*",
    "illuminate/encryption": "5.0.*",
    "illuminate/events": "~5.0@dev",
    "swiftmailer/swiftmailer": "5.0.*",
    "guzzlehttp/guzzle": "4.2.*",
    "cboden/Ratchet": "0.3.*",
    "react/socket": "0.4.*",
    "duccio/apns-php": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*",
    "aws/aws-sdk-php": "2.7.*",
    "dropbox/dropbox-sdk": "1.1.*",
    "opauth/opauth": "dev-master",
    "opauth/twitter": "dev-master",
    "opauth/facebook": "dev-master"
  },
  "repositories": [{
      "type": "vcs",
      "url": "http://github.com/endel/Slim"
  }],
  "scripts": {
    "post-install-cmd": [ ]
  },
  "bin-dir": "bin",
  "minimum-stability": "dev"
}
